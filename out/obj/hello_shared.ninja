defines = -DHELLO_SHARED_IMPLEMENTATION
include_dirs =
cflags = -fPIC -pthread
cflags_cc =
target_out_dir = obj
target_output_name = libhello_shared

build obj/libhello_shared.hello_shared.o: cxx ../hello_shared.cxx

build ./libhello_shared.so: solink obj/libhello_shared.hello_shared.o
  ldflags =
  libs =
  frameworks =
  swiftmodules =
  output_extension = .so
  output_dir = .
